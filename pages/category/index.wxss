page{
    height: 100%;
}
.cates{
    height: 100%;
}

.cates .cates_container{
    height: calc(100vh - 90rpx);
    display: flex;
}

.cates .cates_container .left_menu{
    flex: 2;
    /* background-color: aqua; */
}
.cates .cates_container .left_menu .menu_item{
    font-size: 30rpx;
    height: 80rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}
.cates .cates_container .right_content{
    flex: 5;
    /* background-color: #ff6700; */
}
.cates .cates_container .right_content .goods_group{

}
.cates .cates_container .right_content .goods_group .goods_title{
    height: 80rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}
.cates .cates_container .right_content .goods_group .goods_title .delimiter{
    color: #ccc;
    padding: 0 10px;
}
.cates .cates_container .right_content .goods_group .goods_title .title{
    
}
.cates .cates_container .right_content .goods_group .good_list{
    display: flex;
    flex-wrap: wrap;
}
.cates .cates_container .right_content .goods_group .good_list navigator{
    width: 33.33%;
    text-align: center;
}
.cates .cates_container .right_content .goods_group .good_list navigator image{
    width: 50%;
}
.cates .cates_container .right_content .goods_group .good_list navigator .goods_name{
    
}
.active{
    color: var(--themeColor);
    border-left: 5rpx solid currentColor;
}